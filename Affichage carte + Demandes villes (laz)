void demandevilles ()
{
  printf ("Le tableau de demande des villes est : \n\n");
  int tabdemandes[3][4] = {{2,0,3,1};{3,2,1,0};{0,1,2,3}};
  int i,j;
  for (i=0;i<3;i++)
    {
     printf ("\n\n");
     for (j=0;j<4;j++)
      {
       printf ("%d", tabdemandes[i][j]);
      }
    }
    //return 0;
}


